<div class='row mt-sm-3'>
  <div class="col-sm">
    <%= form_tag("#", method: "get", id: "cloud_form1", class: "mt-3") do %>
      <div class="form-group">
        <div class="row">
          <div class="col-sm-4">
            <%= label :selectDate1, "Data Referência:"%>
            <%= select_tag :selectDate1, options_for_select([202001, 202002, 202003, 202004, 202005]), 
                options = { include_blank: true, class: "form-control state" } %>
          </div>
          <div class="col-sm-4">
            <%= label :selectScore1, "Nota Termômetro:"%>
            <%= select_tag :selectScore1, options_for_select([1, 2, 3, 4, 5]), 
                options = { include_blank: true, class: "form-control state" } %>
          </div>
          <div class="col-sm-4 d-flex align-items-end">
            <%= button_tag("Enviar", class: "btn btn-primary navbar-vivo", id: "enviar1", name: "enviar1") %>
          </div>
        </div>
      </div>
    <% end %>
    <div id="graph1"></div>
  </div>
  <div class="col-sm">
    <%= form_tag("#", method: "get", id: "cloud_form2", class: "mt-3") do %>
      <div class="form-group">
        <div class="row">
          <div class="col-sm-4">
            <%= label :selectDate2, "Data Referência:"%>
            <%= select_tag :selectDate2, options_for_select([202001, 202002, 202003, 202004, 202005]), 
                options = { include_blank: true, class: "form-control state" } %>
          </div>
          <div class="col-sm-4">
            <%= label :selectScore2, "Nota Termômetro:"%>
            <%= select_tag :selectScore2, options_for_select([1, 2, 3, 4, 5]), 
                options = { include_blank: true, class: "form-control state" } %>
          </div>
          <div class="col-sm-4 d-flex align-items-end">
            <%= button_tag("Enviar", class: "btn btn-primary navbar-vivo", id: "enviar2", name: "enviar2") %>
          </div>
        </div>
      </div>
    <% end %>
    <div id="graph2"></div>
  </div>
</div>